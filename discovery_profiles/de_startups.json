{
  "source_name": "DE-Startups_Directory",
  "start_urls": [
    "https://www.eu-startups.com/directory/wpbdp_category/german-startups/"
  ],
  "listing_page_selectors": {
    "company_link": ".wpbdp-listing .listing-title a",
    "pagination_next": ".wpbdp-pagination .next"
  },
  "detail_page_extractors": {
    "company_name": { "selector": "h1.entry-title", "type": "text" },
    "website": { "selector": ".wpbdp-field-website .value", "type": "text" },
    "founded": { "selector": ".wpbdp-field-founded .value", "type": "text" },
    "tags": { "selector": ".wpbdp-field-tags .value", "type": "text" },
    "based_in": { "selector": ".wpbdp-field-based_in .value", "type": "text" },
    "business_description": { "selector": ".wpbdp-field-business_description .value", "type": "text" },
    "long_business_description": { "selector": ".wpbdp-field-long_business_description .value", "type": "text" },
    "category": { "selector": ".wpbdp-field-category .value", "type": "text" },
    "total_funding": { "selector": ".wpbdp-field-total_funding .value", "type": "text" },
    "company_status": { "selector": ".wpbdp-field-company_status .value", "type": "text" }
  },
  "max_pages_to_crawl_per_start_url": 0,
  "request_delay_seconds": 5
}